<Window x:Class="Hdd.WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpfApp="clr-namespace:Hdd.WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        d:DataContext="{d:DesignInstance wpfApp:Vm, IsDesignTimeCreatable=False}">
    <StackPanel>
        <ContentControl Content="{Binding ViewModel1}"/>
        <ContentControl Content="{Binding ViewModel2}"/>
    </StackPanel>
</Window>
